<% if !case_item.nil? %>
<tr>
	<td>	<%= link_to case_item.defendant.to_s, case_item %>	</td>
	<td>	<%= case_item.ccn %> 		</td>
	<td>	<%= case_item.cr %> 		</td>
	<td>	<%= case_item.ca %> 		</td>
	<td>	<%= case_item.cj %> 		</td>
	<td>	<%= case_item.ja %> 		</td>
	<td>	<%= case_item.ct %> 		</td>
	<td>	<%= case_item.sao %> 		</td>
	<td>	<%= link_to "edit", edit_case_path(case_item),
					:method => :edit,
					:class => "btn btn-mini" %>	</td>

	<% if current_user.admin? %>
			<td>	
			<%= link_to "delete", case_item, 	:method => :delete, 
												:confirm => "You sure?",
												:class => "btn btn-mini",
												:title => "delete this case"
												%>							
			</td>
	<%end%>
</tr>
<% end %>