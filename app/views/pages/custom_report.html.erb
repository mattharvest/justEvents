<% if signed_in? %>
	<h1>Your Search Results...</h1>
	<h3><i class="icon-download"></i><%= link_to 'Download as CSV', csv_path(
				:type=>'custom', 
				:start=>@start, 
				:end=>@end,
				:micropost_category=>@category
				) %>
				</h3>
	
	<table class="table table-striped table-bordered table-condensed">
		<thead>
			<tr>
				<th>	Event Date: 	</th>
				<th>	Defendant:		</th>
				<th>	Casenumber:		</th>
				<th>	ASA: 			</th>
				<th>	Unit:			</th>
				<th>	ASA Email: 		</th>
				<th>	D.O.B: 			</th>
				<th>	Attorney: 		</th>
				<th>	Category: 		</th>
				<th>	What Happened: 	</th>
				<th>	Created On: 	</th>
				<th>	Gang:			</th>
				<th>	Jail:			</th>
				<th>	Suspended time:	</th>
				<th>	Probation:		</th>
				<th>	Community Service:	</th>
				<th>	Judge:			</th>
				<th>	Lead Charge:	</th>
				<th>	Convicted Charges:	</th>
				<th>	Enhanced:		</th>
				<th>	Guidelines:		</th>
				<th>	ABA:			</th>
				<th>	Team Leader:	</th>
				<th>	Delete Button:  </th>
			</tr>
		</thead>
		<tbody>
			<%= render :partial => 'shared/post_item', :collection => @microposts %>
		</tbody>
	</table>
	
<% else %>
	<%= render 'shared/introstub' %>
<% end %>