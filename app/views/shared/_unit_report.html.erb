<h2>Unit Report</h2>
<h1><%= current_user.unit %></h1>
	<table class="table table-striped table-bordered table-condensed">
		<thead>
			<tr>
				<th>	Event Date: 	</th>
				<th>	Defendant:		</th>
				<th>	Casenumber:		</th>
				<th>	ASA: 			</th>
				<th>	Unit:			</th>
				<th>	ASA Email: 		</th>
				<th>	D.O.B: 			</th>
				<th>	Attorney: 		</th>
				<th>	Category: 		</th>
				<th>	What Happened: 	</th>
				<th>	Created On: 	</th>
				<th>	Gang:			</th>
				<th>	Executed Sentence:	</th>
				<th>	Suspended Sentence: </th>
				<th>	Probation:		</th>
				<th>	Community Service:	</th>
				<th>	Judge:			</th>
				<th>	Lead Charge:	</th>
				<th>	Convicted Charges:	</th>
				<th>	Enhanced:		</th>
				<th>	Guidelines:		</th>
				<th>	Team Leader:	</th>
	
				<%if current_user.admin? %>
					<th>	Delete Button: 	</th>
				<% end %>
			</tr>
		</thead>
		<tbody>
			<%= render :partial => 'shared/post_item', :collection => @unit_posts %>
		</tbody>
	</table>